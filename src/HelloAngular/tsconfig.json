{
  "compilerOptions": {
    "outDir": "wwwroot/app/",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
